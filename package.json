  "bugs": {
    "url": "https://github.com/barend-erasmus/web-push-service/issues"
  },
  "homepage": "https://github.com/barend-erasmus/web-push-service#readme",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.4",
    "prettier": "^1.14.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "nedb": "^1.8.0",
    "uuid": "^3.3.2",
    "web-push": "^3.3.2"
  },
  "bin": {
    "wsmq": "./bin/wsmq"
