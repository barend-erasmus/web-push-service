{
  "name": "web-push-service",
  "version": "0.0.8",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run build-compile & npm run build-bundle & npm run build-optimize",
    "build-compile": "tsc",
    "build-bundle": "browserify ./dist/libraries/main.js -o ./dist/libraries/main.bundle.js -s WebPushService & browserify ./dist/libraries/service-worker.js -o ./dist/libraries/service-worker.bundle.js -s WebPushServiceServiceWorker",
    "build-optimize": "uglifyjs ./dist/libraries/main.bundle.js -o ./dist/libraries/main.bundle.min.js & uglifyjs ./dist/libraries/service-worker.bundle.js -o ./dist/libraries/service-worker.bundle.min.js",
    "start": "npm run build & node ./dist/main.js start --port 8080",
    "start-demo": "http-server -c -1 -o http://localhost:8081/index.html -p 8081 ./src/libraries",
    "test": "npm run build & node ./dist/tests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barend-erasmus/web-push-service.git"
  },
  "author": "Barend Erasmus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/barend-erasmus/web-push-service/issues"
  },
  "homepage": "https://github.com/barend-erasmus/web-push-service#readme",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/node": "^10.5.5",
    "@types/supertest": "^2.0.5",
    "prettier": "^1.14.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "ajv": "^6.5.2",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "commander": "^2.16.0",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "nedb": "^1.8.0",
    "uuid": "^3.3.2",
    "web-push": "^3.3.2"
  },
  "bin": {
    "web-push-service": "./bin/web-push-service"
  }
}