{
  "name": "web-push-service",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build-demo": "tsc & browserify ./dist/demo/main.js -s WebPushServiceDemo -o ./src/demo/main.bundle.js & uglifyjs ./src/demo/main.bundle.js -o ./src/demo/main.bundle.min.js & browserify ./dist/demo/service-worker.js -s WebPushServiceDemoServiceWorker -o ./src/demo/service-worker.bundle.js & uglifyjs ./src/demo/service-worker.bundle.js -o ./src/demo/service-worker.bundle.min.js",
    "start": "npm run build & node ./dist/main.js start --port 8080",
    "start-demo": "npm run build-demo & http-server -c -1 -o http://localhost:8082/index.html -p 8082 ./src/demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barend-erasmus/web-push-service.git"
  },
